{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MongoDB": "mongodb://localhost:27017/myimage"
  },
  "JwtSettings": {
    "Secret": "MyImage-Super-Secret-JWT-Key-For-Development-2024-Must-Be-At-Least-32-Characters-Long",
    "Issuer": "MyImageAPI",
    "Audience": "MyImageClient",
    "ExpirationHours": 24
  },
  "GridFsSettings": {
    "BucketName": "photos",
    "ChunkSizeBytes": 1048576
  },
  "StripeSettings": {
    "PublishableKey": "pk_test_your_publishable_key_here",
    "SecretKey": "sk_test_your_secret_key_here",
    "WebhookSecret": "whsec_your_webhook_secret_here"
  },
  "FileUploadSettings": {
    "MaxFileSizeBytes": 52428800,
    "AllowedExtensions": [".jpg", ".jpeg"],
    "ThumbnailMaxWidth": 300,
    "ThumbnailMaxHeight": 300
  },
  "CorsSettings": {
    "AllowedOrigins": [
      "http://localhost:4200",
      "https://localhost:4200",
      "http://localhost:3000",
      "http://localhost:5159",
      "https://localhost:7037",
      "http://localhost:8080"
    ]
  }
}